{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "WorkManager Macro",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#control.keywords"
		},
		{
			"include": "#logical.operator.keywords"
		},
		{
			"include": "#other.keywords"
		},
		{
			"include": "#function.entity.names"
		},
		{
			"include": "#double-quoted-strings"
		},
		{
			"include": "#single-quoted-strings"
		},
		{
			"include": "#numberic.constants"
		},
		{
			"include": "#support.functions"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.block.wm",
					"begin": "{",
					"end": "}"
				}
			]
		},
		"control.keywords": {
			"patterns": [{
				"name": "keyword.control.wm",
				"match": "\\b(BREAK|CANCEL|CONTINUE|END|EXIT|RETURN|WAIT|IF|ELSE_IF|ELSE|END_IF|LOOP|EXIT_IF|END_LOOP|REPEAT|UNTIL|WHILE|END_WHILE)\\b"
			}]
		},
		"logical.operator.keywords": {
			"patterns": [{
				"name": "keyword.operator.logical.wm",
				"match": "\\b(AND|EXOR|FALSE|NOT|OR|TRUE)\\b"
			}
			]
		},
		"other.keywords": {
			"patterns": [{
				"name": "keyword.other.wm",
				"match": "\\b(DEFINE|END_DEFINE|LET|LOCAL|PARAMETER)\\b"
			}
			]
		},
		"function.entity.names": {
			"patterns": [{
				"name": "entity.name.function.wm",
				"match": "\\b(ABS|ARCCOS|ARCSIN|ARCTAN|COS|DIV|EXP|INT|LG|LN|MOD|PI|RND|ROT|ROUND|SGN|SIN|SQR|SQRT|TAN|TRUNC)\\b"
			}
			]
		},
		"double-quoted-strings": {
			"name": "string.quoted.double.wm",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.wm",
					"match": "\\$."
				}
			]
		},
		"single-quoted-strings": {
			"name": "string.quoted.single.wm",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.wm",
					"match": "\\$."
				}
			]
		},
		"numberic.constants": {
			"patterns": [
				{
					"name": "constant.numeric.wm",
					"match": "\\b(0x[0-9a-fA-F]+|\\d+)\\b"
				}
			]
		},
		"support.functions":{
			"patterns": [
				{
					"name": "support.function.logicaltables.wm",
					"match": "\\b(CATALOG_TO_LTAB|COLOR_LTAB|CONNECT_TABLE|CONNECT_WINDOW|COPY_LTAB|COPY_LTAB_ALL|COPY_LTAB_COL_NAMES|COPY_LTAB_ROW|COPY_LTAB_TITLES|CREATE_LTAB|CSL_LINE_TO_LTAB|CSL_TO_LTAB|DELETE_LTAB|DELETE_LTAB_ROW|DMS_FIND_ CACHEID_ROW|DMS_GENERATE_ CACHEID_FOR_ ROW_UMBER|DMS_IS_USER_CACHEID|EXPAND_LTAB_INDENT|GET_LAST_PICK_LTAB|GET_LAST_PICK_LTAB_COLUMN|GET_LAST_PICK_LTAB_ROW|GET_LAST_PICK_LTAB_TITLE|HIGHLIGHT_LTAB|INSERT_LTAB_ROW|LOOKUP_BY_CACHEID|LOOKUP_REGISTER|LOOKUP_UNREGISTER|LTAB_COLUMNS|LTAB_COL_NAMES|LTAB_LIST|LTAB_ROWS|LTAB_TITLES|LTAB_TO_CSL|LTAB_TO_CSL_LINE|LTAB_TO_TABSL|POP_DOWN_LTAB|POP_UP_LTAB|READ_LTAB|SAVE_LTAB|SCROLL_LTAB|SEARCH_LTAB|SECURE_LTAB|SELECT_FROM_LTAB|SORT_LTAB|WRITE_LTAB)\\b"
				},
				{
					"name": "support.function.cache.wm",
					"match": "\\b(DMS_APPLY|DMS_CACHE_COPY_ELE|DMS_CACHE_COPY_PKT|DMS_CACHEID_IS_DELETED|DMS_CACHEID_MODE|DMS_DEMOTE|DMS_DROP|DMS_FIND_CACHEID_ROW|DMS_GENERATE_CACHEID_ FOR_ROW_NUMBER|DMS_IS_USER_CACHEID|DMS_OBJECT_LTAB|DMS_OBJECT_TYPE|DMS_PROMOTE|GET_CACHE_DDID|GET_CACHE_VALUE|GET_FIELD_CACHEID|SET_CACHE_VALUE)\\b"
				},
				{
					"name": "support.function.structure.wm",
					"match": "\\b(EXPAND_LTAB_INDENT|STRUCTURE_AT|STRUCTURE_AT_CHILD|STRUCTURE_AT_DISABLE|STRUCTURE_AT_ENABLE|STRUCTURE_AT_PARENT|STRUCTURE_CLOSE|STRUCTURE_DEFAULT_ FORMAT|STRUCTURE_DISABLE|STRUCTURE_ENABLE|STRUCTURE_GET_FORMAT|STRUCTURE_GET_OPTIONS|STRUCTURE_GET_ PREFERENCES|STRUCTURE_GET_VIEWS|STRUCTURE_OPEN|STRUCTURE_SET_FORMAT|STRUCTURE_SET_OPTIONS|STRUCTURE_SET_ PREFERENCES|STRUCTURES_DEFAULTS|STRUCTURES_OPTIONS)\\b"
				},
				{
					"name": "support.function.objectmanipulation.wm",
					"match": "\\b(GET_FILE_URL|ADD_RELATED_FILE|DELETE_RELATED_FILE|DELETE_ALL_RELATED_FILES|GET_RELATED_FILE|GET_RELATED_FILE_URL|DMS_ADD_ACL_ENTRY|DMS_ADD_APP_LST_SEND_TO|DMS_ADD_ATT_COLUMN|DMS_ADD_CLASS_RELATION|DMS_ADD_DD_ENUM_STR|DMS_ADD_ELE_RELATION|DMS_ADD_FILE|DMS_ADD_GROUP_USER|DMS_ADD_TO_ELEMENT_SET|DMS_APPLY|DMS_CACHEID_IS_DELETED|DMS_CACHEID_MODE|DMS_CHANGE_ELEMENT_CLASS|DMS_CHECKPOINT|DMS_CHECK_CHANGE_ELEMENT_CLASS|DMS_CHECK_CHANGE_ELEMENT_STATE|DMS_CHECK_PERMISSION|DMS_CLASS_GET_INFO|DMS_COPY_ELEMENT|DMS_COPY_FROM_DB|DMS_COPY_PACKET|DMS_CREATE_ACL|DMS_CREATE_APP_LIST|DMS_CREATE_CLASS|DMS_CREATE_DD|DMS_CREATE_ELEMENT|DMS_CREATE_GROUP|DMS_CREATE_USER|DMS_DD_CHECK_ATTR_VALUE|DMS_DD_CHECK_VALIDITY|DMS_DD_GET_INFO|DMS_DELETE|DMS_DELETE_FROM_ELEMENT_SET|DMS_DEMOTE|DMS_DROP|DMS_LOAD_ACL|DMS_LOAD_APP_LST_SEND_TO|DMS_LOAD_ATT_COLUMN|DMS_LOAD_CLASS_RELATION|DMS_LOAD_DD_ENUM_STR|DMS_LOAD_ELE_RELATION|DMS_LOAD_FILE|DMS_LOAD_GROUP|DMS_OBJECT_LTAB|DMS_PROMOTE|DMS_READ_VALUE|DMS_REFRESH|DMS_SELECT_ACL|DMS_SELECT_APP_LIST|DMS_SELECT_CLASS|DMS_SELECT_DD|DMS_SELECT_ELEMENT|DMS_SELECT_GROUP|DMS_SELECT_USER|DMS_SEND_ELEMENT_SET|DMS_SEND_ELEMENT_SET_ CHECKPOINT_LEVEL|DMS_SET_VALUE|DMS_SUBSTITUTE_CHILD|DMS_UPDATE_TIMESTAMP|GET_CACHE_VALUE|SET_CACHE_VALUE|SET_LINK_CHECK_DIRECTION|TRANS_APPLY|TRANS_BEGIN|TRANS_CANCEL|TRANS_END|TRANS_END_NO_REORDER)\\b"
				},
				{
					"name": "support.function.query.wm",
					"match": "\\b(QUERY_ADD_CLASS|QUERY_ADD_COLUMN|QUERY_ADD_RESTRICT|QUERY_ADD_SELECT|QUERY_ADD_SORT|QUERY_ADD_TRAVERSAL_CLASS|QUERY_ADD_TRAVERSAL_SELECT|QUERY_ALIAS_COLUMN|QUERY_CLOSE_EDIT|QUERY_COPY|QUERY_CREATE|QUERY_CREATE_ CLASSIFICATION|QUERY_DELETE|QUERY_DELETE_ CLASS|QUERY_DELETE_ COLUMN|QUERY_DELETE_ RESTRICT|QUERY_DELETE_SELECT|QUERY_DELETE_SORT|QUERY_DELETE_TRAVERSAL_CLASS|QUERY_DELETE_TRAVERSAL_SELECT|QUERY_DESTINATION|QUERY_DIRECTION|QUERY_DISTINCT_ROWS|QUERY_END_TIME|QUERY_EXECUTE|QUERY_FIXED_COLUMNS|QUERY_FORMAT_TABLE_NAME|QUERY_HINT|QUERY_INDENT|QUERY_LEVELS|QUERY_LIMIT|QUERY_LINK_CLASS_TYPE|QUERY_LIST|QUERY_OPEN_EDIT|QUERY_REFRESH|QUERY_REFRESH_CLASSES|QUERY_RENAME|QUERY_REORDER_COLUMN|QUERY_RESET_EDIT|QUERY_REVERSE_SORT|QUERY_SAVE|QUERY_SELECT_TABLE_NAME|QUERY_SET_CLASSES|QUERY_SET_HIDDEN_COLUMNS|QUERY_SET_SELECT|QUERY_SET_TRAVERSAL_CLASSES|QUERY_SET_TRAVERSAL_SELECT|QUERY_SORT_TABLE_NAME|QUERY_SQL_STRING|QUERY_START_ELID|QUERY_START_TIME|QUERY_SYSTEM_TABLE|QUERY_TABLE_NAME|QUERY_TREE_CHILD_NODE|QUERY_TREE_ELEMENTS|QUERY_TREE_ELEMENTS_SETUP|QUERY_TREE_PARENT_NODE|QUERY_TYPE|QUERY_USER_TABLE_NAME|QUERY_VIEW_TIME|SQL_QUERY|SQL_QUOTE)\\b"
				},
				{
					"name": "support.function.errorhandlingdebugging.wm",
					"match": "\\b(CHECK_ERROR|ECHO|ERROR_MODE|ERROR_STR|ON_ERROR|TRACE|TRAP_ERROR)\\b"
				},
				{
					"name": "support.function.linkclass.wm",
					"match": "\\b(DMS_ADD_LINK|DMS_CREATE_LINK_CLASS|DMS_CREATE_LINK_CLASS_TYPE|DMS_GET_LINK_CLASS_TYPES_OF_ELEMENT_CLASS|DMS_GET_LINK_LTAB|DMS_LINK_CLASS_GET_INFO|DMS_LINK_CLASS_TYPE_GET_ALL_ATTS|DMS_LINK_CLASS_TYPE_GET_INFO|DMS_LINK_CLASS_TYPES_TO_TABLE|DMS_LOAD_LINKS|DMS_SELECT_LINK_CLASS|DMS_SELECT_LINK_CLASS_TYPE)\\b"
				}
			]
		}
	},
	"scopeName": "source.m"
}